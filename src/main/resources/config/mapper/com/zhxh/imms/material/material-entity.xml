<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhxh.imms.material.entity">
    <resultMap id="Material" type="com.zhxh.imms.material.entity.Material">
        <id property="materialId" column="material_id"/>

        <result property="materialNo" column="material_no"/>
        <result property="materialMaterialTypeId" column="material_material_type_id"/>
        <result property="materialName" column="material_name"/>
        <result property="materialUomId" column="material_uom_id"/>
        <result property="materialWidth" column="material_width"/>
        <result property="materialWeight" column="material_weight"/>
        <result property="materialSizeId" column="material_size_id"/>
        <result property="materialPrice" column="material_price"/>
        <result property="materialColor" column="material_color"/>
        <result property="materialDescription" column="material_description"/>
    </resultMap>

    <resultMap id="MaterialMedia" type="com.zhxh.imms.material.entity.MaterialMedia">
        <id property="materialMediaId" column="material_media_id"/>

        <result property="materialMediaMediaId" column="material_media_media_id"/>
        <result property="materialMediaMaterialId" column="material_media_material_id"/>
    </resultMap>

</mapper>